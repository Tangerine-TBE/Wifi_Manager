<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:id="@+id/contentView"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="vertical">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/similarPhotoItem"
            android:layout_marginTop="10dp"
            android:background="@android:color/white"
            android:layout_width="match_parent"
            android:layout_height="160dp">
            <ImageView
                android:id="@+id/similarPhotoItemIcon"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginTop="20dp"
                android:layout_marginStart="10dp"
                android:src="@mipmap/ic_xiangsi"
                android:layout_width="40dp"
                android:layout_height="40dp"/>
            <TextView
                android:id="@+id/similarPhotoItemTitle"
                app:layout_constraintTop_toTopOf="@id/similarPhotoItemIcon"
                app:layout_constraintStart_toEndOf="@id/similarPhotoItemIcon"
                android:layout_marginStart="10dp"
                android:textColor="@android:color/black"
                android:textSize="18sp"
                android:text="@string/SimilarPictures"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <TextView
                android:id="@+id/similarPhotoItemDes"
                app:layout_constraintStart_toStartOf="@id/similarPhotoItemTitle"
                app:layout_constraintTop_toBottomOf="@id/similarPhotoItemTitle"
                android:text="@string/cleanAlikePhoto"
                android:textSize="12sp"
                android:textColor="#6e6e6e"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <ImageView
                android:id="@+id/similarPhotoItemRightIcon"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@id/similarPhotoItemIcon"
                android:layout_marginEnd="10dp"
                android:src="@mipmap/ic_fold_true"
                android:layout_width="18dp"
                android:layout_height="18dp"/>
            <TextView
                android:id="@+id/similarPhotoItemSize"
                app:layout_constraintEnd_toStartOf="@id/similarPhotoItemRightIcon"
                app:layout_constraintTop_toTopOf="@id/similarPhotoItemIcon"
                android:layout_marginEnd="15dp"
                tools:text="159MB"
                android:textSize="12sp"
                android:textColor="#6e6e6e"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <ImageView
                android:id="@+id/similarPhotoItemImage1"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="15dp"
                android:layout_marginStart="5dp"
                android:layout_marginEnd="5dp"
                app:layout_constraintTop_toBottomOf="@id/similarPhotoItemIcon"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="@id/similarPhotoItemIcon"
                app:layout_constraintEnd_toStartOf="@id/similarPhotoItemImage2"
                android:scaleType="centerCrop"
                android:layout_width="0dp"
                android:layout_height="0dp"/>
            <ImageView
                android:id="@+id/similarPhotoItemImage2"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="15dp"
                android:layout_marginStart="5dp"
                android:layout_marginEnd="5dp"
                app:layout_constraintTop_toBottomOf="@id/similarPhotoItemIcon"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@id/similarPhotoItemImage1"
                app:layout_constraintEnd_toStartOf="@id/similarPhotoItemImage3"
                android:scaleType="centerCrop"
                android:layout_width="0dp"
                android:layout_height="0dp"/>
            <ImageView
                android:id="@+id/similarPhotoItemImage3"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="15dp"
                android:layout_marginStart="5dp"
                android:layout_marginEnd="5dp"
                app:layout_constraintTop_toBottomOf="@id/similarPhotoItemIcon"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@id/similarPhotoItemImage2"
                app:layout_constraintEnd_toStartOf="@id/similarPhotoItemImage4"
                android:scaleType="centerCrop"
                android:layout_width="0dp"
                android:layout_height="0dp"/>
            <ImageView
                android:id="@+id/similarPhotoItemImage4"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="15dp"
                android:layout_marginStart="5dp"
                android:layout_marginEnd="5dp"
                app:layout_constraintTop_toBottomOf="@id/similarPhotoItemIcon"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@id/similarPhotoItemImage3"
                app:layout_constraintEnd_toEndOf="@id/similarPhotoItemRightIcon"
                android:scaleType="centerCrop"
                android:layout_width="0dp"
                android:layout_height="0dp"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/screenshotPhotoItem"
            android:layout_marginTop="10dp"
            android:background="@android:color/white"
            android:layout_width="match_parent"
            android:layout_height="160dp">
            <ImageView
                android:id="@+id/screenshotPhotoItemIcon"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginTop="20dp"
                android:layout_marginStart="10dp"
                android:src="@mipmap/ic_screen"
                android:layout_width="40dp"
                android:layout_height="40dp"/>
            <TextView
                android:id="@+id/screenshotPhotoItemTitle"
                app:layout_constraintTop_toTopOf="@id/screenshotPhotoItemIcon"
                app:layout_constraintStart_toEndOf="@id/screenshotPhotoItemIcon"
                android:layout_marginStart="10dp"
                android:textColor="@android:color/black"
                android:textSize="18sp"
                android:text="@string/Shootme"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <TextView
                android:id="@+id/screenshotPhotoItemDes"
                app:layout_constraintStart_toStartOf="@id/screenshotPhotoItemTitle"
                app:layout_constraintTop_toBottomOf="@id/screenshotPhotoItemTitle"
                android:text="@string/screenshotDes"
                android:textSize="12sp"
                android:textColor="#6e6e6e"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <ImageView
                android:id="@+id/screenshotPhotoItemRightIcon"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@id/screenshotPhotoItemIcon"
                android:layout_marginEnd="10dp"
                android:src="@mipmap/ic_fold_true"
                android:layout_width="18dp"
                android:layout_height="18dp"/>
            <TextView
                android:id="@+id/screenshotPhotoItemSize"
                app:layout_constraintEnd_toStartOf="@id/screenshotPhotoItemRightIcon"
                app:layout_constraintTop_toTopOf="@id/screenshotPhotoItemIcon"
                android:layout_marginEnd="15dp"
                tools:text="159MB"
                android:textSize="12sp"
                android:textColor="#6e6e6e"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <ImageView
                android:id="@+id/screenshotPhotoItemImage1"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="15dp"
                android:layout_marginStart="5dp"
                android:layout_marginEnd="5dp"
                app:layout_constraintTop_toBottomOf="@id/screenshotPhotoItemIcon"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="@id/screenshotPhotoItemIcon"
                app:layout_constraintEnd_toStartOf="@id/screenshotPhotoItemImage2"
                android:scaleType="centerCrop"
                android:layout_width="0dp"
                android:layout_height="0dp"/>
            <ImageView
                android:id="@+id/screenshotPhotoItemImage2"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="15dp"
                android:layout_marginStart="5dp"
                android:layout_marginEnd="5dp"
                app:layout_constraintTop_toBottomOf="@id/screenshotPhotoItemIcon"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@id/screenshotPhotoItemImage1"
                app:layout_constraintEnd_toStartOf="@id/screenshotPhotoItemImage3"
                android:scaleType="centerCrop"
                android:layout_width="0dp"
                android:layout_height="0dp"/>
            <ImageView
                android:id="@+id/screenshotPhotoItemImage3"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="15dp"
                android:layout_marginStart="5dp"
                android:layout_marginEnd="5dp"
                app:layout_constraintTop_toBottomOf="@id/screenshotPhotoItemIcon"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@id/screenshotPhotoItemImage2"
                app:layout_constraintEnd_toStartOf="@id/screenshotPhotoItemImage4"
                android:scaleType="centerCrop"
                android:layout_width="0dp"
                android:layout_height="0dp"/>
            <ImageView
                android:id="@+id/screenshotPhotoItemImage4"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="15dp"
                android:layout_marginStart="5dp"
                android:layout_marginEnd="5dp"
                app:layout_constraintTop_toBottomOf="@id/screenshotPhotoItemIcon"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@id/screenshotPhotoItemImage3"
                app:layout_constraintEnd_toEndOf="@id/screenshotPhotoItemRightIcon"
                android:scaleType="centerCrop"
                android:layout_width="0dp"
                android:layout_height="0dp"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/oldPhotoItem"
            android:visibility="gone"
            android:layout_marginTop="10dp"
            android:background="@android:color/white"
            android:layout_width="match_parent"
            android:layout_height="160dp">
            <ImageView
                android:id="@+id/oldPhotoItemIcon"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginTop="20dp"
                android:layout_marginStart="10dp"
                tools:src="@mipmap/icon_xc"
                android:layout_width="40dp"
                android:layout_height="40dp"/>
            <TextView
                android:id="@+id/oldPhotoItemTitle"
                app:layout_constraintTop_toTopOf="@id/oldPhotoItemIcon"
                app:layout_constraintStart_toEndOf="@id/oldPhotoItemIcon"
                android:layout_marginStart="10dp"
                android:textColor="@android:color/black"
                android:textSize="18sp"
                tools:text="相似图片"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <TextView
                android:id="@+id/oldPhotoItemDes"
                app:layout_constraintStart_toStartOf="@id/oldPhotoItemTitle"
                app:layout_constraintTop_toBottomOf="@id/oldPhotoItemTitle"
                tools:text="清理多余的相似图片"
                android:textSize="12sp"
                android:textColor="#6e6e6e"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <ImageView
                android:id="@+id/oldPhotoItemRightIcon"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@id/oldPhotoItemIcon"
                android:layout_marginEnd="10dp"
                android:src="@mipmap/ic_fold_true"
                android:layout_width="18dp"
                android:layout_height="18dp"/>
            <TextView
                android:id="@+id/oldPhotoItemSize"
                app:layout_constraintEnd_toStartOf="@id/oldPhotoItemRightIcon"
                app:layout_constraintTop_toTopOf="@id/oldPhotoItemIcon"
                android:layout_marginEnd="15dp"
                tools:text="159MB"
                android:textSize="12sp"
                android:textColor="#6e6e6e"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <ImageView
                android:id="@+id/oldPhotoItemImage1"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="15dp"
                android:layout_marginStart="5dp"
                android:layout_marginEnd="5dp"
                app:layout_constraintTop_toBottomOf="@id/oldPhotoItemIcon"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="@id/oldPhotoItemIcon"
                app:layout_constraintEnd_toStartOf="@id/oldPhotoItemImage2"
                android:scaleType="centerCrop"
                android:layout_width="0dp"
                android:layout_height="0dp"/>
            <ImageView
                android:id="@+id/oldPhotoItemImage2"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="15dp"
                android:layout_marginStart="5dp"
                android:layout_marginEnd="5dp"
                app:layout_constraintTop_toBottomOf="@id/oldPhotoItemIcon"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@id/oldPhotoItemImage1"
                app:layout_constraintEnd_toStartOf="@id/oldPhotoItemImage3"
                android:scaleType="centerCrop"
                android:layout_width="0dp"
                android:layout_height="0dp"/>
            <ImageView
                android:id="@+id/oldPhotoItemImage3"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="15dp"
                android:layout_marginStart="5dp"
                android:layout_marginEnd="5dp"
                app:layout_constraintTop_toBottomOf="@id/oldPhotoItemIcon"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@id/oldPhotoItemImage2"
                app:layout_constraintEnd_toStartOf="@id/oldPhotoItemImage4"
                android:scaleType="centerCrop"
                android:layout_width="0dp"
                android:layout_height="0dp"/>
            <ImageView
                android:id="@+id/oldPhotoItemImage4"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="15dp"
                android:layout_marginStart="5dp"
                android:layout_marginEnd="5dp"
                app:layout_constraintTop_toBottomOf="@id/oldPhotoItemIcon"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@id/oldPhotoItemImage3"
                app:layout_constraintEnd_toEndOf="@id/oldPhotoItemRightIcon"
                android:scaleType="centerCrop"
                android:layout_width="0dp"
                android:layout_height="0dp"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <FrameLayout
            android:id="@+id/frameLayout"
            android:layout_gravity="center_horizontal"
            android:layout_width="300dp"
            android:layout_height="wrap_content"/>
    </LinearLayout>
</ScrollView>
