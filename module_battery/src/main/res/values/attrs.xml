<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--自定义加载框-->
    <declare-styleable name="LVCircularRing">
        <attr name="bgColor" format="color" />
        <attr name="barColor" format="color" />
    </declare-styleable>

    <declare-styleable name="WaveViewDouble">
        <attr name="waveWaterLevel" format="float" />
        <attr name="amplitudeRatio" format="float" />
        <attr name="waveLengthRatio" format="float" />
        <attr name="waveShiftRatio" format="float" />
        <attr name="frontWaveColor" format="color" />
        <attr name="behindWaveColor" format="color" />
        <attr name="waveShape_" format="enum">
            <enum name="circle" value="0" />
            <enum name="square" value="1" />
        </attr>
        <attr name="showWave" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CircleProgressView">
        <attr name="inner_background" format="color"/>
        <attr name="circle_stroke_width" format="dimension"/>
        <attr name="progressColor" format="color"/>
        <attr name="progressBgColor" format="color"/>
    </declare-styleable>
</resources>